<?xml version='1.0' encoding='utf-8'?>
<package format="3">

  <name>qp_oases</name>
  <version>0.1.24</version>
  <description>Library for quadratic programming using online active set strategy</description>
  <author email="joachim.ferreau@ch.abb.com">Hans Joachim Ferreau</author>
  <author email="potschka@iwr.uni-heidelberg.de">Andreas Potschka</author>
  <author email="christian.kirches@iwr.uni-heidelberg.de">Christian Kirches</author>
  <maintainer email="psankar@anybotics.com">Prajish Sankar</maintainer>
  <maintainer email="jjunger@anybotics.com">Jonas Junger</maintainer>
  <license>LGPLv2.1</license>
  <url type="website">https://github.com/anybotics/qpOASES</url>
  <buildtool_depend condition="$ROS_VERSION == 1">catkin</buildtool_depend>
  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake</buildtool_depend>
  <test_depend condition="$ROS_VERSION == 1">gtest</test_depend>
